{% extends "__secondary_nav.html.j2" %}


{% block secondary_nav_title %}
  PLACEHOLDER SECONDARY NAV TITLE
{% endblock secondary_nav_title %}

{% block admin_cog %}
{% endblock admin_cog %}

{% block nav2_subtitle %}
  PLACEHOLDER SECONDARY NAV SUBTITLE
{% endblock nav2_subtitle %}

{% block content %}
  {# The content and tab names in the secondary name bar #}
  <div class="row">
    <div class="col">
      <div class="card">
        <div class="card-header bg-primary mb-3">
          <ul class="nav nav-tabs card-header-tabs">
            {% block tab_navs %}
              THIS IS ALL JUST A PLACEHOLDER FOR TABNAVS
              {#  Like the following:
              {{ MACRO_MAKE_TAB("my-tab1", true, "Tab 1 Name") }}
              {{ MACRO_MAKE_TAB("my-tab2", false, "Tab 2 Name") }}
              #}
            {% endblock tab_navs %}
          </ul>
        </div>
        <div class="card-body">
          <div class="tab-content" id="tab-content">
            {% block tab_content %}
              THIS IS ALL JUST A PLACEHOLDER FOR TABCONTENT:
              {# Like the following:
              {{ MACRO_MAKE_TAB_CONTENT("my-tab1", true, "test_tab1_content.html.js") }}
              {{ MACRO_MAKE_TAB_CONTENT("my-tab2", false, "test_tab2_content.html.js") }}
              #}
            {% endblock tab_content %}
          </div>
        </div>
      </div>
    </div>
  </div>
{% endblock content %}


{% block my_javascripts %}
  {{ super() }}
{% endblock my_javascripts %}